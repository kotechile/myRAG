# General Configuration
PORT=8080
FLASK_ENV=production
# Set to 'true' to enable embedding optimization (requires memory)
USE_EMBEDDING_OPTIMIZATION=false

# Supabase Configuration
SUPABASE_URL=your_supabase_url
SUPABASE_KEY=your_supabase_service_role_key
SUPABASE_DATABASE_PASSWORD=your_db_password
# Ensure this connection string is for the session pooler (port 5432 or 6543)
DB_CONNECTION2=postgresql://postgres.user:password@host:5432/postgres

# LLM Providers
OPENAI_API_KEY=sk-...
DEEPSEEK_API_KEY=sk-...
LLAMA_CLOUD_API_KEY=llx-...

# Optional
LINKUP_API_KEY=
